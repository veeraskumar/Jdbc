<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<!-- This is Add --> 
	<servlet>
		<servlet-name>Add</servlet-name>
		<servlet-class>calculator.app.AddNumberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/add-page</url-pattern>
	</servlet-mapping>
	
	<!-- This is Sub --> 
	<servlet>
		<servlet-name>Sub</servlet-name>
		<servlet-class>calculator.app.SubNumberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Sub</servlet-name>
		<url-pattern>/sub-page</url-pattern>
	</servlet-mapping>
	
		<!-- This is Multi --> 
	<servlet>
		<servlet-name>Multi</servlet-name>
		<servlet-class>calculator.app.MultiNumberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Multi</servlet-name>
		<url-pattern>/multi-page</url-pattern>
	</servlet-mapping>
	
		<!-- This is div --> 
	<servlet>
		<servlet-name>Div</servlet-name>
		<servlet-class>calculator.app.DivNumberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Div</servlet-name>
		<url-pattern>/div-page</url-pattern>
	</servlet-mapping>
	
		<!-- This is Modules --> 
	<servlet>
		<servlet-name>Modu</servlet-name>
		<servlet-class>calculator.app.ModNumberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Modu</servlet-name>
		<url-pattern>/mod-page</url-pattern>
	</servlet-mapping>
</web-app>
